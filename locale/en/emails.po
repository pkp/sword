msgid "emails.swordDepositNotification.subject"
msgstr "Deposit Notification"

msgid "emails.swordDepositNotification.body"
msgstr ""
"Congratulations on the acceptance of your submission, \"{$submissionTitle}\", for publication in {$contextName}. If you choose, you may automatically deposit your submission in one or more repositories.<br/>\n"
"<br/>\n"
"Go to {$swordDepositUrl} to proceed.<br/>\n"
"<br/>\n"
"This email was generated by the Public Knowledge Project SWORD deposit plugin."

msgid "emails.swordDepositNotification.description"
msgstr "This email template is used to notify an author of optional deposit points for SWORD deposits."

msgid "emails.swordDepositNotification.name"
msgstr "SWORD deposit notification"
